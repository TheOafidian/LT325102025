{
  "name": "tailwindcard",
  "version": "1.0.0",
  "main": ".eleventy.js",
  "scripts": {
    "build:css": "npx tailwindcss -i site/assets/css/styles.css -o _site/assets/css/style.css --minify",
    "watch:css": "npx tailwindcss -i site/assets/css/styles.css -o _site/assets/css/style.css --watch",
    "build": "pnpm run build:css && eleventy",
    "serve": "pnpm run build:css && eleventy --serve",
    "watch": "pnpm run watch:css & eleventy --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "postcss-cli": "^11.0.0",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4"
  }
}
